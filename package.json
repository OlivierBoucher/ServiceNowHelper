{
  "name": "servicenowhelper",
  "version": "1.0.0",
  "description": "Helper for ServiceNow development",
  "main": "main.js",
  "scripts": {
    "start": "npm run buildAppAndClient && electron dist/main.js",
    "buildApp": "gulp",
    "buildClient": "cd app && npm run build",
    "buildAppAndClient": "npm run buildClient && npm run buildApp",
    "installClient": "cd app && npm install",
    "postinstall": "npm run installClient"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "electron-prebuilt": "^0.36.7",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-sourcemaps": "^1.6.0"
  }
}
